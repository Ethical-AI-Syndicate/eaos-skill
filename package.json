{
  "name": "eaos",
  "version": "1.0.0",
  "description": "Enterprise AI Operating System - Multi-agent governance, auditing, compliance, and strategy orchestration for Claude Code",
  "type": "module",
  "main": "cli/eaos.js",
  "bin": {
    "eaos": "./cli/eaos.js"
  },
  "scripts": {
    "start": "node cli/eaos.js",
    "init": "node cli/eaos.js init",
    "status": "node cli/eaos.js status",
    "audit": "node cli/eaos.js audit full",
    "test": "node --test tests/*.test.js",
    "test:validate": "node scripts/validate.js",
    "lint": "eslint cli/ scripts/",
    "format": "prettier --write .",
    "prepare": "husky install",
    "precommit": "npm run test:validate && npm run lint"
  },
  "keywords": [
    "claude-code",
    "ai-agents",
    "enterprise",
    "governance",
    "compliance",
    "soc2",
    "iso27001",
    "nist",
    "devops",
    "automation"
  ],
  "author": "Ethical AI Syndicate",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethical-ai-syndicate/eaos-skill.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "date-fns": "^3.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "ora": "^8.0.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "prettier": "^3.1.1"
  }
}
